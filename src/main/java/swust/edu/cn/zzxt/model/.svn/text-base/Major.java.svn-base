package swust.edu.cn.zzxt.model;

public class Major {
    private Integer majrId;

    private String majrName;

    private String majrShortname;

    private Integer majrInstid;

    private String majrEducationsystem;

    private String majrLevel;

    private String majrDescription;

    public Integer getMajrId() {
        return majrId;
    }

    public void setMajrId(Integer majrId) {
        this.majrId = majrId;
    }

    public String getMajrName() {
        return majrName;
    }

    public void setMajrName(String majrName) {
        this.majrName = majrName;
    }

    public String getMajrShortname() {
        return majrShortname;
    }

    public void setMajrShortname(String majrShortname) {
        this.majrShortname = majrShortname;
    }

    public Integer getMajrInstid() {
        return majrInstid;
    }

    public void setMajrInstid(Integer majrInstid) {
        this.majrInstid = majrInstid;
    }

    public String getMajrEducationsystem() {
        return majrEducationsystem;
    }

    public void setMajrEducationsystem(String majrEducationsystem) {
        this.majrEducationsystem = majrEducationsystem;
    }

    public String getMajrLevel() {
        return majrLevel;
    }

    public void setMajrLevel(String majrLevel) {
        this.majrLevel = majrLevel;
    }

    public String getMajrDescription() {
        return majrDescription;
    }

    public void setMajrDescription(String majrDescription) {
        this.majrDescription = majrDescription;
    }
}