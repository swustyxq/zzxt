				<div class="page-header">
					<small><span class="glyphicon glyphicon-home"></span>&nbsp;&nbsp;&nbsp;&nbsp;当前位置导航：&nbsp;&nbsp;&nbsp;&nbsp;
						<a href="#">首页</a>&gt;<a href="#">国家助学金</a>&gt;<a href="#">申请国家助学金</a>
						</span> </small>
				</div>
			</div>


			<!-- 左侧导航栏    -->
			<div class="col-md-3">
				<div class="bs-example">
					<div id="navigation" class="panel-group"></div>
				</div>
			</div>
			<div class="col-md-9">
				<div class="bs-Shaozhou">

					<div style="text-align: center">
						<h2>国家助学金申请表信息</h2>
					</div>

					<div>
						<table class="table table-bordered" align="center">

							<tr>
								<th
									style="text-align: center; vertical-align: middle; width: 6px"
									rowspan="5">本人情况</th>
							</tr>

							<tr>
								<th>姓名</th>
								<td id="stu-name"></td>
								<th>性别</th>
								<td id="stu-sex"></td>
								<th>出生年月</th>
								<td id="stu-birthday"></td>
								<td rowspan="2" style="vertical-align: middle; width: 100px">照片</td>
							</tr>

							<tr>
								<th>民族</th>
								<td id="stu-nation"></td>
								<th>政治面貌</th>
								<td id="politicState"></td>
								<th>入学时间</th>
								<td id="stu-admissionTime"></td>
							</tr>

							<tr>
								<th>身份证号码</th>
								<td id="stu-idNumber" colspan="3"></td>
								<th>联系电话</th>
								<td id="stuphone" colspan="2"></td>
							</tr>
							<tr>
								<td colspan="3"><b>西南科技大学</b><span
									id="stu-class-discription"></span><span id="stu-class-name"></span><b>班</b></td>
								<th>年度</th>
								<td id="annual" ></td>
								<th>助学金等级</th>
								<td><select id="level" class="form-control">
										<option selected="selected" value="1">一档助学金</option>
										<option value="2">二档助学金</option>
										<option value="3">三档助学金</option>
								</select></td>
							</tr>

							<tr>
								<th
									style="text-align: center; vertical-align: middle; width: 6px"
									rowspan="4">家庭经济情况</th>
							</tr>


							<tr>
								<th>家庭户口</th>
								<td id="residence" colspan="2"></td>

								<th>家庭人口总数</th>
								<td><input id="familySum" style="width: 100%" type="text"
									class="form-control" value=""></td>

								<th>申请状态</th>
								<td id="station" style="color: red">未申请</td>
							</tr>

							<tr>
								<th>家庭月总收入</th>
								<td><input id="allIncomes" style="width: 100%" type="text"
									class="form-control" value=""></td>
								<th colspan="2">人均月总收入</th>
								<td><input id="personalIncomes" style="width: 100%"
									type="text" class="form-control" value=""></td>
								<th>收入来源</th>
								<td><input id="incomeType" style="width: 100%" type="text"
									class="form-control" value=""></td>
							</tr>



							<tr>
								<th>家庭住址</th>

								<td id="home" colspan="4"></td>

								<th>邮政编码</th>
								<td id="mailcode"></td>
							</tr>

							<tr>
								<th
									style="text-align: center; vertical-align: middle; width: 6px"
									rowspan="5">家庭成员情况</th>
							</tr>

							<tr>
								<th>姓名</th>
								<th>角色</th>
								<th colspan="2">与本人关系</th>
								<th colspan="3">工作单位</th>
							</tr>



							<tr>
								<td id="gzxj_pareName1">&nbsp;</td>
								<td id="gzxj_pareRole1">&nbsp;</td>
								<td id="gzxj_pareRelation1" colspan="2">&nbsp;</td>
								<td id="gzxj_pareWork1" colspan="3">&nbsp;</td>
							</tr>

							<tr>
								<td id="gzxj_pareName2">&nbsp;</td>
								<td id="gzxj_pareRole2">&nbsp;</td>
								<td id="gzxj_pareRelation2" colspan="2">&nbsp;</td>
								<td id="gzxj_pareWork2" colspan="3">&nbsp;</td>
							</tr>
							<tr>
								<td id="gzxj_pareName3">&nbsp;</td>
								<td id="gzxj_pareRole3">&nbsp;</td>
								<td id="gzxj_pareRelation3" colspan="2">&nbsp;</td>
								<td id="gzxj_pareWork3" colspan="3">&nbsp;</td>
							</tr>

							<tr>
								<th>申请理由</th>
								<td colspan="8"><textarea id="applyReason"
										class="form-control" rows="3" placeholder="申请理由"></textarea></td>
							</tr>
							<tr id="classtable" style="display: none">
								<th>班级意见</th>
								<td colspan="8"><textarea id="classOption"
										class="form-control" rows="3" placeholder="班级意见"></textarea></td>
							</tr>

							<tr id="academytable" style="display: none">
								<th>学院意见</th>
								<td colspan="8"><textarea id="academyOption"
										class="form-control" rows="3" placeholder="学院意见"></textarea></td>
							</tr>
						</table>
					</div>
					<div id="fanhui" style="display: block" style="position: relative;">
						<div class="col-md-12" align="center">
							<a href="/zzxt/htmls/seeZxList.html?pareid=10&funid=41"><button
									class="btn btn-info" type="button">
									<span class="glyphicon glyphicon-arrow-left"></span> 返回
								</button></a>
						</div>
					</div>

					<div id="gongneng" style="display: block"
						style="position: relative;">
						<div class="col-md-12" align="center">
							<button id="saveGjzxj" class="btn btn-sm btn-primary"
								type="submit" onclick="javascript:audit(0)">保存</button>
							&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
							<button id="comitApplyGjzxj" class="btn btn-sm btn-primary"
								type="submit" onclick="javascript:audit(1)">审批通过</button>
							&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
							<button id="returnGjzxj" class="btn btn-sm btn-primary"
								type="submit" onclick="javascript:audit(2)">驳回</button>
							&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <a
								href="/zzxt/htmls/seeZxList.html?pareid=10&funid=41"><button
									id="saveGjzxj" class="btn btn-sm btn-primary" type="button">返回</button></a>

						</div>
					</div>
					<br /> <br />
				</div>
			</div>
		</div>
