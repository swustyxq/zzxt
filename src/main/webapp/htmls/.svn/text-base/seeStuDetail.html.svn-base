</head>

<body>

	<!-- Fixed navbar -->
	<div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
		<div class="container">
			<div class="navbar-header">
				<a class="navbar-brand" href="index.html">西南科技大学学生资助系统</a>
			</div>
			<div class="navbar-collapse collapse">
				<ul class="nav navbar-nav navbar-right">
					<li class="dropdown"><a href="#" class="dropdown-toggle"
						data-toggle="dropdown"><span class="glyphicon glyphicon-user"></span><span
							id="current_user">学生用户</span> </a>
						<ul class="dropdown-menu">
							<li class="divider"></li>
							<li class="dropdown-header">系统帮助</li>
							<li><a href="about.html"><span
									class="glyphicon glyphicon-book"></span>关于系统</a></li>
							<li><a href="help.html"><span
									class="glyphicon glyphicon-print"></span>使用说明</a></li>
							<li class="divider"></li>
							<li class="dropdown-header">基本功能</li>
							<li><a href="#" data-toggle="modal"
								data-target="#changePassword"><span
									class="glyphicon glyphicon-cog"></span>修改密码</a></li>
							<li><a href="#" id="logout"><span
									class="glyphicon glyphicon-log-out"></span>退出</a></li>
						</ul></li>
				</ul>
			</div>
			<!--/.nav-collapse -->
		</div>
	</div>
	<div class="container theme-showcase">
		<div class="col-md-12">
			<div class="col-md-12">

				<div class="page-header">
					<small><span class="glyphicon glyphicon-home"></span>&nbsp;&nbsp;&nbsp;&nbsp;当前位置导航：&nbsp;&nbsp;&nbsp;&nbsp;
						<span id="location"> <a href="#">首页</a>&gt;<a href="#">使用说明</a>&gt;<a
							href="#">学生信息</a>
					</span> </small>
				</div>
			</div>


			<!-- 左侧导航栏    -->
			<div class="col-md-3">
				<div class="bs-example">
					<div id="navigation" class="panel-group"></div>
				</div>
			</div>
			<div class="col-md-9">
				<div class="bs-Shaozhou">
					<h2 align="center">学生详细信息</h2>

					<ul id="myTab" class="nav nav-tabs" role="tablist">
						<li class="active"><a data-toggle="tab" role="tab"
							href="#personal">个人信息</a></li>
						<li class=""><a data-toggle="tab" role="tab" href="#Reward">获奖情况</a>
						</li>
						<li class=""><a data-toggle="tab" role="tab" href="#parents">家庭成员</a>
						</li>
						<li class=""><a data-toggle="tab" role="tab" href="#learing">学习情况</a>
						</li>
						<li class=""><a data-toggle="tab" role="tab"
							href="#lookIfoDeatil">详细资料</a></li>
					</ul>
					<div id="myTabContent" class="tab-content">
						<div id="personal" class="tab-pane fade active in">
							<!-- 个人信息添加 -->
							<hr />
							<p align=center style="color: blue; font-size: 25px;">个人信息</p>
							<table class="table table-striped table-bordered"
								id="stuPersonal">

							</table>
						</div>
						<div id="lookIfoDeatil" class="tab-pane fade">
							<!-- 详细信息添加 -->
							<hr />
							<p align=center style="color: blue; font-size: 25px;">个人详细资料</p>
							<table class="table table-striped table-bordered" id="stuInfo">

							</table>
						</div>
						<div id="Reward" class="tab-pane fade">
							<!-- <p>获奖情况内容添加于此</p> -->
							<hr />
							<p align=center style="color: blue; font-size: 25px;">获奖情况</p>
							<table class="table table-striped table-bordered" id="stuReward">

							</table>
						</div>
						<div id="parents" class="tab-pane fade">
							<!-- 父母信息添加于此 -->
							<hr />
							<p align=center style="color: blue; font-size: 25px;">父母信息</p>
							<table class="table table-striped table-bordered" id="stuParents">

							</table>
						</div>

						<div id="learing" class="tab-pane fade">
							<!-- 学习情况添加于此 -->
							<hr />
							<p align=center style="color: blue; font-size: 25px;">学习情况</p>
							<table class="table table-striped table-bordered" id="stuLearing">

							</table>
						</div>
					</div>

				<div align="center">
						<button type="button" class="btn btn-info"
							onclick="javascript: history.go(-1);">
							<span class="glyphicon glyphicon-arrow-left"></span> 返回
						</button>
					</div>
				</div>
			</div>

		</div>
		<!-- 修改密码的模态对话框 Modal -->
		<div class="modal fade" id="changePassword" tabindex="-1"
			role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal">
							<span aria-hidden="true">&times;</span><span class="sr-only">Close</span>
						</button>
						<h4 class="modal-title">修改用户密码</h4>
					</div>
					<div class="modal-body">
						<!-- 对话框里面的内容   -->
						<form class="form-horizontal" role="form">
							<div class="form-group">
								<label for="inputEmail3" class="col-sm-4 control-label">当前密码：</label>
								<div class="col-sm-6">
									<input type="password" class="form-control" id="inputPassword1"
										placeholder="原始密码">
								</div>
							</div>
							<div class="form-group">
								<label for="inputPassword3" class="col-sm-4 control-label">新密码：</label>
								<div class="col-sm-6">
									<input type="password" class="form-control" id="inputPassword2"
										placeholder="新密码">
								</div>
							</div>
							<div class="form-group">
								<label for="inputPassword3" class="col-sm-4 control-label">再次输入新密码：</label>
								<div class="col-sm-6">
									<input type="password" class="form-control" id="inputPassword3"
										placeholder="重复新密码">
								</div>
							</div>
						</form>
					</div>
					<div class="modal-footer">
						<button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
						<button Id="changePWD" type="button" class="btn btn-primary">保存</button>
					</div>
				</div>
			</div>
		</div>
	</div>


	<footer class="bs-footer" role="contentinfo">
		<p class="pull-right">
			<a href="#">回到页面顶部</a>
		</p>
		<div class="container" id="copyright"></div>
	</footer>
</body>
</html>
